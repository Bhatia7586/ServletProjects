<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ServletListenerDemo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MySessionListenerServlet</display-name>
    <servlet-name>MySessionListenerServlet</servlet-name>
    <servlet-class>pack.MySessionListenerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MySessionListenerServlet</servlet-name>
    <url-pattern>/srv1</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CountListenerServlet</display-name>
    <servlet-name>CountListenerServlet</servlet-name>
    <servlet-class>pack.CountListenerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CountListenerServlet</servlet-name>
    <url-pattern>/srv2</url-pattern>
  </servlet-mapping>
  <listener>
  	<listener-class>pack.CountListener</listener-class>
  </listener>
</web-app>